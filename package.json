{
  "name": "kaskadi-textbox",
  "version": "1.0.0",
  "description": "",
  "main": "kaskadi-textbox.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "example": "xdg-open localhost:1234/example && serve -l 1234",
    "test": "npm run lint && npm run test.All",
    "test.FF": "karma start --single-run --browsers FirefoxHeadless karma.conf.js",
    "test.Chrome": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test.All": "karma start --single-run --browsers ChromeHeadless,FirefoxHeadless karma.conf.js",
    "lint": "standard --fix",
    "kaskadi-init": "kaskadi init && git add -u && git commit -m 'initialized from template'",
    "coverage": "npm run lint && npm run test.Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaskadi/kaskadi-textbox.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kaskadi/kaskadi-textbox/issues"
  },
  "homepage": "https://github.com/kaskadi/kaskadi-textbox#readme",
  "kaskadi": {
    "s3-push": {
      "files": [
        {
          "src": "kaskadi-textbox.js",
          "dest": "modules/@kaskadi/kaskadi-textbox/{branch}kaskadi-textbox.js"
        },
        {
          "src": "example/index.html",
          "dest": "modules/@kaskadi/kaskadi-textbox/{branch}example/index.html"
        }
      ]
    }
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "karma": "^5.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.2",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^7.1.1",
    "standard": "^14.3.3"
  },
  "dependencies": {}
}
